{% extends 'base.html.twig' %}

{% block title %}Ticket Details
{% endblock %}

{% block body %}

	<div class="container mt-2">
		<h1>Ticket Details</h1>
		<p>
			<strong>ID:</strong>
			{{ ticket.getId() }}</p>
